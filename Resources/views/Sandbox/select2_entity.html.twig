{% extends 'RJMGenemuSandboxBundle::sandbox_layout.html.twig' %}

{% block stylesheets %}
    {{ parent() }}

    {{ form_stylesheet(form) }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    {{ form_javascript(form) }}
{% endblock %}

{% block content %}
    <h1>genemu_jqueryselect2_entity (no AJAX)</h1>

    <form action="{{ path('rjm_genemu_sandbox_jqueryselect2_entity') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}

        <input type="submit" />
    </form>
{% endblock %}